<h2>Admin Zone</h2>

<h3>User List</h3>

<ul>
    {{#each userArray}}
    <li>
        <h4>{{this.fullName}}</h4>
        <ul>
            <li>Email: {{this.email}} </li>
            <li>Role: {{this.role}} </li>
            <li>Registered: {{this.updatedAt}} </li>
            <li></li>

        </ul>
    </li>

    {{/each}}
</ul>